{
  "name": "minify-ts",
  "version": "1.3.0",
  "description": "A minifier based on typescript compiler to safely rename all of the variables, classes, functions, properties and methods to short.",
  "type": "module",
  "module": "./lib/index.min.js",
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.min.js"
    }
  },
  "bin": {
    "minify-ts": "./lib/cli.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/foreverflying/minify-ts.git"
  },
  "keywords": [
    "typescript",
    "minify"
  ],
  "author": "Andy Deng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foreverflying/minify-ts/issues"
  },
  "homepage": "https://github.com/foreverflying/minify-ts#readme",
  "files": [
    "./lib/*"
  ],
  "dependencies": {
    "commander": "^14.0.2",
    "source-map": "^0.7.4",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "bye-esm-ext": "^1.0.5",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.22.0",
    "eslint-plugin-import": "^2.31.0",
    "rollup": "^4.54.0",
    "terser": "^5.44.1"
  }
}
