{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Minify Self",
      "smartStep": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/out/cli.js",
      "runtimeArgs": [
        "--import=bye-esm-ext",
      ],
      "args": [
        "src",
        "min/src",
        "index.ts",
        "cli.ts"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Minify",
      "smartStep": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMaps": true,
      "program": "${workspaceFolder}/out/try/tryMinify.js",
      "runtimeArgs": [
        "--import=bye-esm-ext",
      ],
      "preLaunchTask": "tsc: build - tsconfig.json"
    },
  ]
}